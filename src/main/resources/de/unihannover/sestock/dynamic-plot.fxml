<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="317.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.unihannover.sestock.PlotController">
   <children>
      <VBox alignment="CENTER" prefHeight="521.0" prefWidth="600.0">
         <children>
            <Label fx:id="stockTitle" alignment="CENTER" prefHeight="17.0" prefWidth="419.0" text="Stock Chart" textAlignment="CENTER" />
            <LineChart fx:id="LineChartStock" prefHeight="413.0" prefWidth="600.0">
              <xAxis>
                <CategoryAxis side="BOTTOM" fx:id="x" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="y" side="LEFT" />
              </yAxis>
            </LineChart>
            <Button fx:id="CloseStockChartButton" mnemonicParsing="false" onAction="#onCloseButtonClick" text="Close window" />
         </children>
      </VBox>
   </children>
</AnchorPane>
