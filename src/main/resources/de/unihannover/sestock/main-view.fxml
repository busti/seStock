<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="233.0" prefWidth="422.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.unihannover.sestock.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <ComboBox fx:id="stockName" editable="true" onKeyReleased="#onStockNameChange" prefWidth="150.0" promptText="Kursnamen..." />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
   <Button fx:id="plotterBtn" mnemonicParsing="false" onAction="#handlePlotterBtnClick" text="Plotter" />
</VBox>
